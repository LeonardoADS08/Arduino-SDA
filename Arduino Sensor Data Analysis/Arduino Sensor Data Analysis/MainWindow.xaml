<Window x:Class="Arduino_Sensor_Data_Analysis.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Arduino_Sensor_Data_Analysis"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <DataGrid x:Name="dataGrid" HorizontalAlignment="Left" Margin="30,23,0,0" VerticalAlignment="Top" Height="164" Width="166"/>
        <Label x:Name="label" Content="Label" HorizontalAlignment="Left" Margin="57,213,0,0" VerticalAlignment="Top"/>
        <Label x:Name="label1" Content="Label" HorizontalAlignment="Left" Margin="57,244,0,0" VerticalAlignment="Top"/>
        <Label x:Name="label2" Content="Label" HorizontalAlignment="Left" Margin="57,275,0,0" VerticalAlignment="Top" RenderTransformOrigin="-0.526,0.231"/>
        <Button x:Name="button" Content="Button" HorizontalAlignment="Left" Margin="121,219,0,0" VerticalAlignment="Top" Width="75" Click="button_Click"/>
        <TextBlock x:Name="textBlock" HorizontalAlignment="Left" Height="164" Margin="201,23,0,0" TextWrapping="Wrap" Text="TextBlock" VerticalAlignment="Top" Width="200"/>

    </Grid>
</Window>
